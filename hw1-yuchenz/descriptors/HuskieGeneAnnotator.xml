<?xml version="1.0" encoding="UTF-8"?>
<analysisEngineDescription xmlns="http://uima.apache.org/resourceSpecifier">
  <frameworkImplementation>org.apache.uima.java</frameworkImplementation>
  <primitive>true</primitive>
  <annotatorImplementationName>edu.cmu.cs.lti.huskie.HuskieGeneAnnotator</annotatorImplementationName>
  <analysisEngineMetaData>
    <name>Huskie Gene Annotator</name>
    <description>Test</description>
    <version>1.0</version>
    <vendor>Yuchen Zhang</vendor>
    <configurationParameters/>
    <configurationParameterSettings/>
    <typeSystemDescription>
      <imports>
        <import location="GeneAnnotatorTypeSystem.xml"/>
      </imports>
    </typeSystemDescription>
    <typePriorities/>
    <fsIndexCollection/>
    <capabilities>
      <capability>
        <inputs/>
        <outputs>
          <type>edu.cmu.cs.lti.huskie.GeneAnnotation</type>
          <feature>edu.cmu.cs.lti.huskie.GeneAnnotation:gene</feature>
          <feature>edu.cmu.cs.lti.huskie.GeneAnnotation:sentence_id</feature>
          <feature>edu.cmu.cs.lti.huskie.GeneAnnotation:end</feature>
          <feature>edu.cmu.cs.lti.huskie.GeneAnnotation:begin</feature>
        </outputs>
        <languagesSupported/>
